{
  "name": "hydro-bot",
  "version": "2.1.3",
  "bin": "bin/robot.js",
  "main": "dist/main.js",
  "engines": {
    "node": ">13.10"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "debug": "node dist/hack && node --async-stack-traces --enable-source-maps --experimental-vm-modules --expose-gc --inspect --trace-deprecation --trace-warnings bin/robot.js",
    "start": "node dist/hack && node --enable-source-maps --experimental-vm-modules --expose-gc bin/robot.js",
    "lint": "eslint bin src --ext ts,js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydro-dev/HydroBot.git"
  },
  "author": "masnn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydro-dev/HydroBot/issues"
  },
  "dependencies": {
    "ansi_up": "^4.0.4",
    "axios": "^0.21.1",
    "fastest-levenshtein": "^1.0.12",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.1",
    "koishi-plugin-gosen-choyen": "^0.2.1",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "pngjs": "^6.0.0",
    "regexpp": "^3.1.0",
    "rss-feed-emitter": "^3.2.2",
    "sharp": "^0.27.0",
    "superagent": "^6.1.0",
    "superagent-proxy": "^2.1.0"
  },
  "homepage": "https://github.com/masnn/qqbot#readme",
  "description": "A simple qq robot",
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.166",
    "@types/mongodb": "^3.5.33",
    "@types/node": "^14.14.17",
    "@types/pngjs": "^3.4.2",
    "@types/puppeteer-core": "^5.4.0",
    "@types/sharp": "^0.26.1",
    "@types/superagent": "4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1"
  }
