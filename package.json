    "ansi_up": "^4.0.4",
    "axios": "^0.20.0",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.0",
    "koishi-adapter-cqhttp": "^1.0.5",
    "koishi-core": "^2.3.1",
    "koishi-plugin-chess": "^2.0.0-beta.11",
